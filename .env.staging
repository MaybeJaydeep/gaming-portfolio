# Staging environment variables
REACT_APP_ENV=staging
REACT_APP_API_URL=https://staging-api.jaydeep-portfolio.com
REACT_APP_ANALYTICS_ID=GA_STAGING_ID
REACT_APP_SENTRY_DSN=YOUR_STAGING_SENTRY_DSN
REACT_APP_VERSION=$npm_package_version

# Build optimizations (less aggressive for debugging)
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=10000

# Performance monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_WEB_VITALS=true

# Feature flags
REACT_APP_ENABLE_SOUND=true
REACT_APP_ENABLE_ANIMATIONS=true
REACT_APP_ENABLE_PARTICLES=true

# Debug flags
REACT_APP_DEBUG_MODE=true
