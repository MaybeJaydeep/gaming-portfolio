# Development environment variables
REACT_APP_ENV=development
REACT_APP_API_URL=http://localhost:3001
REACT_APP_VERSION=$npm_package_version

# Build settings for development
GENERATE_SOURCEMAP=true
FAST_REFRESH=true

# Performance monitoring (disabled in dev for performance)
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false
REACT_APP_ENABLE_WEB_VITALS=false

# Feature flags
REACT_APP_ENABLE_SOUND=true
REACT_APP_ENABLE_ANIMATIONS=true
REACT_APP_ENABLE_PARTICLES=true

# Debug flags
REACT_APP_DEBUG_MODE=true
REACT_APP_SHOW_PERFORMANCE_OVERLAY=true
